{
  "description": "A very lightweight tab suspender to suspend inactive tabs that reduces an overall memory usage of firefox, uses a firefox native discard api",
  "manifest_version": 2,
  "name": "New Tab Suspender",
  "version": "1.6",
  "homepage_url": "https://github.com/pradeep-mishra/tab_suspender_firefox",
  "author": "Pradeep Mishra",
  "icons": {
    "48": "icons/border-48.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "sessions",
    "*://*/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "58.0",
      "id": "{477aa089-9abb-438b-ba8b-4a713827289c}"
    }
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "New Tab Suspender",
    "default_popup": "popup.html"
  }
}